{
  "name": "trem_plus",
  "version": "0.0.1",
  "description": "Taiwan Real-time Earthquake Monitoring ( 臺灣即時地震監測 )",
  "main": "main.js",
  "homepage": "https://exptech.com.tw/",
  "author": "ExpTechTW <ExpTech.tw@gmail.com>",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder",
    "lint": "eslint --ext .js js/"
  },
  "build": {
    "productName": "TREM",
    "appId": "tw.exptech.trem_plus",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": null,
      "target": [],
      "icon": "TREM.ico"
    },
    "linux": {
      "publish": null,
      "target": "deb",
      "category": "Development",
      "maintainer": "ExpTechTW"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dev"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM-Plus.git"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0"
  },
  "dependencies": {}
}
